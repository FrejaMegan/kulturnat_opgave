---
const videoSrc = "https://carild.dk/kea/08/video/herovideo.mp4";
---
<section class="hero-container">
    <video autoplay loop muted playsinline class="hero-video">
      <source src={videoSrc} type="video/mp4" />
    </video>
  </section>
  <style>
    .hero-container {
      position: relative;
      width: 100%;
      height: 100vh;
      overflow: hidden;
    }
  
    .hero-video {
      width: 100%;
      height: 100%;
      object-fit: cover;
    }
  </style>